{
    "name": "blog",
    "version": "1.0.0",
    "private": true,
    "description": "This is a express boilerplate",
    "main": "index.js",
    "scripts": {
        "lint": "eslint --fix --ext .js .",
        "dev": "nodemon --exec babel-node src/core/bin/www.js",
        "start": "node ./dist/core/bin/www.js",
        "clean": "rm -rf dist && mkdir dist",
        "build:babel": "babel ./src --out-dir dist --copy-files",
        "build": "npm run clean && npm run build:babel",
        "seed": "babel-node ./src/package/mogp/core/seeding.js",
        "migrate": "babel-node ./src/package/mogp/core/migration.js",
        "rollback": "babel-node ./src/package/mogp/core/rollback.js",
        "data:prepare": "npm run build && npm run rollback && npm run migrate && npm run seed"
    },
    "dependencies": {
        "@babel/runtime": "^7.15.4",
        "bcrypt": "^5.0.1",
        "cloudinary": "^1.28.1",
        "cookie-parser": "~1.4.4",
        "cors": "^2.8.5",
        "debug": "~2.6.9",
        "dotenv": "^10.0.0",
        "express": "~4.16.1",
        "express-fileupload": "^1.3.1",
        "glob": "^7.2.0",
        "http-status": "^1.5.0",
        "joi": "^17.4.2",
        "jsonwebtoken": "^8.5.1",
        "lodash": "^4.17.21",
        "method-override": "^3.0.0",
        "mongoose": "^6.0.13",
        "socket.io": "^4.4.0",
        "swagger-ui-express": "^4.1.6",
        "winston": "^3.3.3"
    },
    "devDependencies": {
        "@babel/cli": "^7.10.4",
        "@babel/core": "^7.10.4",
        "@babel/node": "^7.12.10",
        "@babel/plugin-proposal-class-properties": "^7.10.4",
        "@babel/plugin-proposal-decorators": "^7.12.1",
        "@babel/plugin-syntax-dynamic-import": "^7.8.3",
        "@babel/plugin-transform-async-to-generator": "^7.10.4",
        "@babel/plugin-transform-runtime": "^7.10.4",
        "@babel/preset-env": "^7.10.4",
        "@babel/register": "^7.10.5",
        "babel-eslint": "^10.1.0",
        "babel-plugin-module-resolver": "^4.1.0",
        "eslint": "^7.3.1",
        "eslint-config-airbnb-base": "^14.2.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-import": "^2.21.2",
        "eslint-plugin-prettier": "^3.4.0",
        "nodemon": "^2.0.13",
        "prettier": "^2.3.0",
        "pug": "^3.0.2"
    }
}
